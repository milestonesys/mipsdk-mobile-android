<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.milestonesys.mobilesdk.audiosample">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:label="@string/app_name"
        android:icon="@mipmap/iva_icon"
        android:roundIcon="@mipmap/iva_icon_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AudioSample"
        android:allowBackup="false"
        android:usesCleartextTraffic="true">
        <activity
            android:name="com.milestonesys.mobilesdk.audiosample.ui.view.MainActivity"
            android:configChanges="orientation|screenSize|screenLayout|smallestScreenSize|keyboardHidden"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>