<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.milestone.playbacksample">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name=".SDKSampleApplication"
        android:icon="@mipmap/iva_icon"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:usesCleartextTraffic="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".CameraListActivity"
            android:label="@string/camera_list_title" />
        <activity
            android:name=".PlaybackActivity"
            android:label="@string/playback_title" />
    </application>
</manifest>